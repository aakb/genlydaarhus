{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */
#}
<input{{ attributes.addClass('image-upload--file js-image-upload') }} />{{ children }}

{% set button = {
  "name": attributes.id,
  "titleEmpty": "fusk"
} %}

{% include "@elements/buttons/button-label-call-to-action.twig" with { button: button } %}
