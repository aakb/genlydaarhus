<nav class="hamburger-menu js-hamburger-menu {{ is_front ? 'is-transparent' }}">
  <ul class="hamburger-menu--links">
    <li class="hamburger-menu--group">
      {% for item in hamburgermenuItems %}
        <a href="{{ item.url }}" class="hamburger-menu--link">
          {% include "@elements/icons/icon.twig" with {icon: { type: "chevron", class: "navigation--icon-transparent is-tiny is-white has-small-margin" }} %}
          {{ item.title }}
        </a>
      {% endfor %}
    </li>
  </ul>
</nav>